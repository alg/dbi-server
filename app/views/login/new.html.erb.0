<!-- begin content --><table>
<!-- form definition begins -->
<%= form_tag("/login", :method=>'post', :multipart=>true)  do %> 
                    <tbody><tr><th colspan="2">Please log in to access your DeterLab account</th>
                    </tr>
		    <tr>
                        <td>Username:</td>
                        <td>
			  <%= text_field_tag 'uid', '', :maxlength => 16, :size => 16 %> 
			</td>
                    </tr>
		    <tr>
                        <td>Password:</td> 
                        <td>
			  <%= password_field_tag 'password', '', :maxlength => 16, :size => 16, :disabled => true %>
			</td>
                    </tr>
		    <tr>
                        <td colspan="2" align="center">
                            <b> <%= submit_tag "Login" %> </b>
                         </td>
                    </tr>
<% end %>                 
<!-- form definition ends -->
             </tbody></table>
